{
  "name": "@scstanton20/tago-analysis-worker",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=24.3.0"
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm run dev:backend\"",
    "dev:frontend": "pnpm --filter \"frontend\" run dev",
    "dev:backend": "pnpm --filter \"backend\" run dev",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "build-types": "pnpm --filter \"types\" build"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-yml": "^1.19.0",
    "globals": "^16.4.0",
    "prettier": "3.6.2"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6"
